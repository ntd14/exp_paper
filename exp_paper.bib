	author = {Asnacios, Atef and Hamant, Olivier},
	month = nov,
	year = {2012},
	keywords = {cytoskeleton, endocytosis, extracellular matrix, membrane tension, osmotic pressure, Stiffness},
	pages = {584--591}
}

